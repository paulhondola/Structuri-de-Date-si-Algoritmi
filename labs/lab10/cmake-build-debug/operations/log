OPEN files/file1.txt
PRINT files/file1.txt
WRITE files/file1.txt 5 12345
PRINT files/file1.txt
DELETE files/file1.txt 20 7
PRINT files/file1.txt
WRITE files/file1.txt 100 *******
PRINT files/file1.txt
SAVEAS files/file1.txt files/ceva.txt
PRINT files/file1.txt
PRINT files/ceva.txt

